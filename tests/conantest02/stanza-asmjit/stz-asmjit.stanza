;For use with `within'

public defn gen-code (f: (CodeHolder, Assembler) -> ?, rt:JitRuntime, show-size?:True|False, disassembling?:True|False, out:FileOutputStream) -> Func :
  val code = code-holder-new(rt)
  val assembler = assembler-new(code)
  f(code, assembler)
  val func = add(rt, code)
  if disassembling? or show-size? :
    flatten(code)
    println(out, "SIZE: %_" % [size(code)]) when show-size?
    ;disassemble(value(func), size(code), out) when disassembling?
    flush(out)
  delete(assembler)
  reset(code)
  delete(code)
  func

