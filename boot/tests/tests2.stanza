defpackage user :
   import core
   import collections

defn f (x, y) -> Int :
   x as Int|String
   y as Int|String
   match(x, y) :
      (x:Int, y:String) : 0
      (x:Int, y:Int) : 1
      (x:String, y:String) : 2
      (x:String, y:String) : 3   

defn hello () :
   val v = Vector<String>()
   add-all(v, ["Hello" "World"])
   println(v)
   
public defn main () :
   println(f(30, "Hello"))
   println(f("Hello", "World"))
   println(f(10, 30))
   println(f("Hello", 10))

hello()
main()