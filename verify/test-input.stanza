#include<"core/stringeater.stanza">
#include<"compiler/stz-lexer.stanza">
#include("compiler/stz-il-ir.stanza")
#include("compiler/stz-input.stanza")

defpackage stz/test-input :
   import core
   import verse
   import stz/il-ir
   import stz/input
   import stz/lexer

defn main () :
   val filename = commandline-arguments()[1]
   val lexed = unwrap-token(lex-file(filename))
   for form in lexed do :
      println("Reading:")
      println(form)
      println("Result:")
      println(read(form))
      println("\n")

main()