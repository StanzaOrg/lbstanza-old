;                   Void Check Insertion
;                   ====================

defn void-check-insertion (prog:KProg) :
   val void-var = HashTable<Int,True>({_})

   ;Find all global variables that may be void.
   for c in comms(prog) do :
      match(c) :
         (c:KDef) : void-var[n(c)] = true
         (c:KDefVar) : void-var[n(c)] = true
         (c:KLSDef) : void-var[n(c)] = true
         (c) : false

   ;Insert void checks when necessary
   defn check-void (e:KExp) :
      match(e) :
         (e:KLet) :
            ;Note as voidable if no default value for variable
            match(def(e)) :
               (def:KDefVar) :
                  if value(def) == false :
                     void-var[n(def)] = true
               (def) : false
            mapr(check-void, e)
         (e:KVar) :
            if key?(void-var, n(e)) :
               error("Untested.")
               KCheckVoid(e, false)
            else : e
         (e:KTupleGet) :
            error("Untested.")
            KCheckVoid(mapr(check-void, e), false)
         (e) :
            mapr(check-void, e)

   ;Insert void checks for all HiStanza constructs
   val comms* = for c in comms(prog) map :
      map(check-void, c)
   KProg(comms*)   


;                    Mutable Variables Boxing
;                    ========================

defn box-mutables (prog:KProg) :
   val mtable = HashTable<Int, True>({_})
   defn box! (v:Int) : mtable[v] = true
   defn box? (v:Int) : get?(mtable, v, false)

   defn box (e:KExp) :
      match(e) :
         (e:KLet) :
            match(def(e)) :
               (d:KDefVar) :
                  error("Untested.")
                  box!(n(d))
                  val d* = map(box, d) as KDefVar
                  val body* = box(body(e))
                  KLet(KDef(n(d), KBox(value(d*), false)),
                       body*,
                       false)
               (d) :
                  mapr(box, e)
         (e:KSet) :
            val v* = box(value(e))
            if box?(n(e)) :
               error("Untested.")
               KBoxSet(KVar(n(e), false), v*, false)
            else : KSet(n(e), v*, false)
         (e:KVar) :
            if box?(n(e)) :
               error("Untested.")
               KBoxGet(KVar(n(e), false), false)
            else : e
         (e) :
            mapr(box, e)

   ;Driver
   KProg $ for c in comms(prog) map :
      map(box, c)


;                    Method TArg Mapping
;                    ===================

defn sub (t:KType, env:List<KeyValue<Int,KType>>) :
   match(t) :
      (t:KTVar) :
         val e = for e in env find : key(e) == n(t)
         match(e) :
            (e:KeyValue<Int,KType>) : value(e)
            (e) : t
      (t) :
         map(sub{_, env}, t)

defn method-targ-mapping (prog:KProg) :
   ;Compute current type table, and multi types
   val tt = TypeTable()
   val multis = HashTable<Int, KDefmulti>({_})
   for c in comms(prog) do :
      match(c) :
         (c:KDefType) : add-type(tt, c)
         (c:KLSDefType) : add-ls-type(tt, c)
         (c:KDefmulti) : multis[n(c)] = c
         (c) : false
   end-adds(tt)      
   
   ;Solve: xs = ts(ys) for ys
   defn solve-inverse (xs:List<Int>, ys:List<Int>, ts:List<KType>) :
      ;Find solutions
      val xs-table = HashTable<Int, KType>({_})
      val solns = HashTable<Int, List<Int>>({_})
      defn find-soln (x:Int, t:KType) :
         match(t) :
            (t:KTVar) : solns[n(t)] = List(x, get?(solns,n(t),List()))
            (t:KOr) : (find-soln(x, a(t)), find-soln(x, b(t)))
            (t) : false
      for (x in xs, t in ts) do :
         xs-table[x] = t
         find-soln(x, t)
         
      ;Find minimal solutions
      for y in ys map :
         if key?(solns, y) :
            val xs = minima(solns[y], subtype?{tt, xs-table[_], xs-table[_]})
            reduce(KAnd, stream(KTVar, xs))
         else : KTop()

   ;Replace type arguments in function
   defn replace-targs (f:KFn, targs*:List<Int>, env:List<KeyValue<Int, KType>>) :
      val a1* = for t in a1(f) map : sub(t, env)
      val a2* = sub(a2(f), env)
      val body* = mapr(sub{_, env}, body(f))
      KFn(n(f), tail?(f), targs*, a1*, a2*, args(f), body*)

   ;Map methods over commands
   defn map-methods-comm (c:KComm) :
      match(map(map-methods-exp, c)) :
         (c:KDefmethod) :
            if empty?(targs(c)) :
               c
            else :
               error("Untested.")
               ;Compute appropriate mapping to method type arguments
               var func* = let :
                  val targs* = map(fresh-id{}, targs(c))
                  val mapping = solve-inverse(targs*, targs(func(c)), targs(c))
                  val env = map(KeyValue, targs(func(c)), mapping)
                  replace-targs(func(c), targs*, env)
               ;Inherit multi guards
               func* = let :
                  val m = multis[multi(c)]
                  val env = map({_ => KTVar(_)}, targs(m), targs(func*))
                  val a1* = for (x in a1(m), y in a1(func*)) map :
                     KAnd(sub(x, env), y)
                  val a2* = KAnd(sub(a2(m), env), a2(func*))
                  KFn(n(func*), tail?(func*), targs(func*), a1*, a2*, args(func*), body(func*))
               ;Return new defmethod
               KDefmethod(n(c), multi(c), targs(c), thisn(c), func*)            
         (c) : c
   defn map-methods-exp (e:KExp) :
      map(map-methods-exp, map(map-methods-comm, e))

   ;Driver
   KProg $ for c in comms(prog) map :
      map-methods-comm(c)


;                    Callsite Normalization
;                    ======================

defn callsite-normalization (prog:KProg) :
   defn normalizable? (e:KExp) :
      e typeof KLet|KLetRec|KSeq|KDispatch|KMatch|KMix

   defn normalize (f:KExp, targs:List<KType>, arg-vs:List<KExp>, a1:List<KType>, a2:KType) :
      defn loop (f:KExp) -> KExp :
         match(f) :
            (f:KLet) :
               KLet(def(f), loop(body(f)), false)
            (f:KLetRec) :
               KLetRec(defs(f), loop(body(f)), false)
            (f:KSeq) :
               KSeq(a(f), loop(b(f)), false)
            (f:KDispatch) : 
               val branches* = for b in branches(f) map : map(loop, b)
               KDispatch(args(f), branches*, false)
            (f:KMatch) :
               val branches* = for b in branches(f) map : map(loop, b)
               KMatch(args(f), branches*, false)
            (f) :
               KDo(f, targs, arg-vs, a1, a2, false)
      loop(f)
         
   defn convert-exp (e:KExp) :
      match(mapr(convert-exp, e)) :
         (e:KDo) :
            if normalizable?(func(e)) :
               error("Untested.")
               ;Force arguments to immediate form
               val defs = Vector<KDef>()
               val args* = for a in args(e) map :
                  match(a) :
                     (a:KVar|KLiteral) :
                        a
                     (a) :
                        val n = fresh-id()
                        add(defs, KDef(n, a))
                        KVar(n, false)
               ;Normalize function using arguments         
               var exp* = normalize(func(e), targs(e), args*, a1(e), a2(e))
               for d in in-reverse(defs) do :
                  exp* = KLet(d, exp*, false)
               exp*   
            else : e
         (e) : e

   KProg $ for c in comms(prog) map :
      map(convert-exp, c)


;                   Global Closure Conversion
;                   =========================
   
defn convert-global-closures (prog:KProg) :
   ;Discover all global functions
   val functions = HashTable<Int,True>({_})
   for c in comms(prog) do :
      match(c) :
         (c:KDefn) : functions[n(c)] = true
         (c:KDefmulti) : functions[n(c)] = true
         (c:KLSDefn) : functions[n(c)] = true
         (c) : false

   ;Mapping from closure entries to their global variables
   val accum = Vector<KComm>()
   val closure-map = HashTable<Int, Int>({_})
   defn closure-var (n:Int) :
      error("Untested.")
      if not key?(closure-map, n) :
         val n* = fresh-id()
         add(accum, KDef(n*, KCurry(n, List(), false)))
         closure-map[n] = n*
      closure-map[n]

   ;Convert all references to HiStanza expression
   defn convert-exp (e:KExp) :
      match(e) :
         (e:KDo) :
            val args* = map(convert-exp, args(e))
            val func* = match(func(e)) :
               (f:KVar) : f
               (f) : convert-exp(f)
            KDo(func*, targs(e), args*, a1(e), a2(e), false)
         (e:KVar) :
            if key?(functions, n(e)) :
               KVar(closure-var(n(e)), false)
            else : e
         (e) :
            mapr(convert-exp, e)

   ;Convert all references in LoStanza expressions
   defn convert-lsexp (e:KLSExp) :
      match(e) :
         (e:KLSVar) :
            if key?(functions, n(e)) :
               KLSVar(closure-var(n(e)))
            else : e
         (e) :
            mapr(convert-lsexp, e)
   defn convert-comm (c:KLSComm) :
      mapr(convert-lsexp, c)

   ;Driver
   for c in comms(prog) do :
      add{accum, _} $
      map{convert-comm, _} $
      map{convert-lsexp, _} $
      map{convert-exp, _} $
      c      
   KProg(to-list(accum))      


;                    KMix/Curry Conversion
;                    =====================

defstruct FEntry :
   targs: List<Int>
   a1: List<KType>
   a2: KType
   
defn mix-curry-conversion (prog:KProg) :
   ;Discover all mixable functions and their types
   val functions = HashTable<Int, FEntry>({_})
   defn record-entry (c:KComm) :
      match(c) :
         (c:KDefn) :
            val f = value(c) as KFn
            functions[n(c)] = FEntry(targs(f), a1(f), a2(f))
         (c:KDefmulti) :
            functions[n(c)] = FEntry(targs(c), a1(c), a2(c))
         (c:KLSDefn) :
            defn ref? (t:KLSType) : t typeof KRefT
            defn ref-type (t:KLSType) : type(t as KRefT)
            if all?(ref?, a1(c)) and ref?(a2(c)) :
               val a1* = map(ref-type, a1(c))
               val a2* = ref-type(a2(c))
               functions[n(c)] = FEntry(targs(c), a1*, a2*)
         (c) : false
   do(record-entry, comms(prog))

   defn func-arity (n:Int) :
      length(a1(functions[n]))

   defn func-types (n:Int, ts:List<KType>) :
      val entry = functions[n]
      val env = map(KeyValue, targs(entry), ts)
      val a1* = for t in a1(entry) map :
         sub(t, env)
      val a2* = sub(a2(entry) env)
      [a1*, a2*]

   ;Reference position
   defn to-func (e:KMix|KCurry) -> Func :
      defn kfn (arity:Int, f:KExp) :         
         val args* = to-list(stream(fresh-id{}, 0 to arity))
         val arg-vs = map(KVar, args*)
         val a1* = map(KTop{}, args*)
         val a2* = KTop()
         val call* = conv-exp(KDo(f, List(), arg-vs, a1*, a2*, false))
         KFn(fresh-id(), true, List(), a1*, a2*, args*, call*)
      match(e) :
         (e:KCurry) :
            error("Untested.")
            kfn(func-arity(n(e)), e)
         (e:KMix) :
            error("Untested.")
            val groups = HashTable<Int, List<KCurry>>({_})
            for e in exps(e) do :
               val arity = func-arity(n(e))
               groups[arity] = List(e, get?(groups, arity, List()))
            KMultifn{fresh-id(), to-list(_)} $
            for e in groups stream :
               kfn(key(e), KMix(value(e), false))      

   ;Convert all mix/curry nodes in expression
   defn conv-exp (e:KExp) :      
      match(e) :
         ;Calling position Mix/Curry
         (e:KDo) :
            match(func(e)) :
               (f:KCurry) :
                  error("Untested.")
                  empty?(targs(e)) as True ;Assertion
                  val args* = map(conv-exp, args(e))
                  val [a1*, a2*] = func-types(n(f), targs(f))
                  KDo(KVar(n(f)), targs(f), args*, a1*, a2*, false)
               (f:KMix) :
                  error("Untested.")
                  empty?(targs(e)) as True ;Assertion
                  val args* = map(conv-exp, args(e))
                  val bs* = for f in exps(f) map :
                     val ns = map(fresh-id{}, args*)
                     val arg-vs = map(KVar, ns)
                     val [a1*, a2*] = func-types(n(f), targs(f))
                     val call = KDo(KVar(n(f)), targs(f), arg-vs, a1*, a2*, false)
                     KBranch(ns, a1*, call)
                  KDispatch(args*, bs*, false)
               (f) :
                  mapr(conv-exp, e)
         ;Reference position curry
         (e:KMix|KCurry) :
            error("Untested.")
            val n = fresh-id()
            KLet(KDefn(n, to-func(e)),
                 KVar(n),
                 false)
         ;Otherwise recurse
         (e) :
            mapr(conv-exp, e)            

   ;Convert all mix/curry nodes in LoStanza expression
   defn conv-lsexp (e:KLSExp) :
      match(e) :
         ;Calling position Mix/Curry
         (e:KLSCall) :
            match(func(e)) :
               (f:KLSCurry) :
                  error("Untested.")
                  empty?(targs(e)) as True ;Assertion
                  val args* = map(conv-lsexp, args(e))
                  val [a1*, a2*] = func-types(n(f), targs(f))
                  KLSCall(KLSVar(n(f)), targs(f), args*, type(e))
               (f:KLSMix) :
                  error("Untested.")
                  empty?(targs(e)) as True ;Assertion
                  val ret = fresh-id()
                  val args* = map(conv-lsexp, args(e))
                  val bs* = for f in exps(f) map :
                     val ns = map(fresh-id{}, args*)
                     val arg-vs = map(KLSVar, ns)
                     val [a1*, a2*] = func-types(n(f), targs(f))
                     val call = KLSCall(KLSVar(n(f)), targs(f), arg-vs, type(e))
                     val set-comm = KLSSet(Value(KLSVar(ret)), call)
                     KLSBranch(ns, a1*, set-comm)
                  KLSLet(
                     KLSDispatch(args*, bs*),
                     KLSVar(ret))                     
               (f) :
                  map(conv-lsexp, e)
         ;Reference position curry
         (e:KLSMix|KLSCurry) :
            error("Untested.")
            defn to-curry (e:KLSCurry) : KCurry(n(e), targs(e), false)
            val e* = match(e) :
               (e:KLSCurry) : to-curry(e)
               (e:KLSMix) : KMix(map(to-curry, exps(e)), false)
            FuncExp(to-func(e*))         
         ;Otherwise recurse         
         (e) :
            mapr(conv-lsexp, e)
   defn conv-comm (c:KLSComm) :
      mapr(conv-lsexp, c)

   ;Driver
   KProg $ for c in comms(prog) map :
      map{conv-comm, _} $
      map{conv-lsexp, _} $
      map{conv-exp, _} $
      c


;                       Boundary Guards
;                       ===============
                     
defn boundary-guard-insertion (prog:KProg) :
   ;Which functions have guards?
   val args-guarded = HashTable<Int, True>({_})
   val ret-guarded = HashTable<Int, True>({_})
   val multi-guarded = HashTable<Int, True|False>({_})
   defn args-guarded! (n:Int) : args-guarded[n] = true
   defn args-guarded? (n:Int) : key?(args-guarded, n)
   defn ret-guarded! (n:Int) : ret-guarded[n] = true
   defn ret-guarded? (n:Int) : key?(ret-guarded, n)
   defn multi-guarded! (n:Int, v:True|False) :
      multi-guarded[n] = v and get?(multi-guarded, n, true)

   ;     Annotate all Guard Information
   ;     ------------------------------
   defn annotate-guard-info (n:Int, f:Func) :
      args-guarded!(n)
      match(f) :
         (f:KFn) : ret-guarded!(n) when not tail?(f)
         (f:KMultifn) : ret-guarded!(n) when none?(tail?, funcs(f) as List<KFn>)

   defn annotate-exp (e:KExp) :
      do(annotate-exp, e)
      do(annotate-comm, e)
         
   defn annotate-comm (c:KComm) :
      match(c) :
         (c:KDefn) :
            args-guarded!(n(c))
            match(value(c)) :
               (f:KFn) : ret-guarded!(n(c)) when not tail?(f)
               (f:KMultifn) : ret-guarded!(n(c)) when none?(tail?, funcs(f) as List<KFn>)            
         (c:KDefmulti) :
            multi-guarded!(n(c), true)
         (c:KDefmethod) :
            multi-guarded!(multi(c), not tail?(func(c)))            
         (c) : false
      do(annotate-exp, c)

   ;Scan through top-level functions
   do(annotate-comm, comms(prog))
   for entry in multi-guarded do :
      args-guarded!(key(entry))
      ret-guarded!(key(entry)) when value(entry)

   ;     Guard Insertion
   ;     ---------------
   ;Convert all expressions
   defn conv-exp (e:KExp) :   
      match(map(conv-exp, map(conv-comm, e))) :
         (e:KDo) :
            defn guard-args () :
               val args* = map(KCast{_, _, false}, args(e), a1(e))
               KDo(func(e), targs(e), args*, a1(e), a2(e), false)   
            defn guard-ret (exp:KExp) :
               KCast(exp, a2(e), false)
               
            match(func(e)) :
               (f:KVar) :
                  var e* = if args-guarded?(n(f)) : e
                           else : guard-args()
                  if ret-guarded?(n(f)) : e*
                  else : guard-ret(e*)
               (f) :
                  guard-ret(guard-args())
         (e) : e

   ;Insert guards for function
   defn conv-fn<?T> (f:?T&Func) -> T :
      {_ as T&Func} $ match(f) :
         (f:KFn) :
            defn tail-cast (e:KExp, t:KType) :
               match(e) :
                  (e:KCast) : KCast(exp(e), KAnd(t, targ(e)), false)
                  (e:KLet) : KLet(def(e), tail-cast(body(e), t), false)
                  (e:KLetRec) : KLetRec(defs(e), tail-cast(body(e), t), false)
                  (e:KSeq) : KSeq(a(e), tail-cast(b(e), t), false)                  
                  (e) : KCast(e, t, false)

            ;Recursively convert sub expressions
            var body* = conv-exp(body(f))
            
            ;Guard arguments
            for (a in args(f), t in a1(f)) do :
               body* = KSeq(KCast(KVar(a), t, false), body*, false)
               
            ;Guard return
            body* =
               if tail?(f) : tail-cast(body*, a2(f))
               else : KCast(body*, a2(f), false)
            KFn(n(f), tail?(f), targs(f), a1(f), a2(f), args(f), body*)            
         (f:KMultifn) :
            KMultifn(n(f), map(conv-fn, funcs(f)))

   ;Convert LoStanza expressions/commands
   defn conv-ls-exp (e:KLSExp) :
      match(e) :
         (e:FuncExp) : FuncExp(conv-fn(func(e)))
         (e) : mapr(conv-ls-exp, e)
   defn conv-ls-comm (c:KLSComm) :
      mapr(conv-ls-exp, c)

   ;Convert top level commands
   defn conv-comm (c:KComm) :
      match(c) :
         (c:KDefn) :
            KDefn(n(c), conv-fn(value(c)))
         (c:KDefmethod) :
            KDefmethod(n(c), multi(c), targs(c), thisn(c), conv-fn(func(c)))
         (c) : map(conv-exp, map(conv-ls-comm, c))

   ;Driver
   KProg(map(conv-comm, comms(prog)))


      
;                    Type Reification
;                    ================

defn type-reification (prog:KProg) :
   ;Create type objects
   val accum = Vector<KComm>()

   defn def-type-obj (t:KType, vs:List<Int>) :
      val n = fresh-id()
      val vs* = map(fresh-id{}, vs)
      val t* = sub(t, map({_ => KTVar(_)}, vs, vs*))
      add(accum, KDefTypeObject(n, vs*, t*))
      n      

   defn make-type-obj (t:KType) :
      defn freevars (t:KType) :
         generate<Int> :
            defn loop (t:KType) :
               match(t) :
                  (t:KTVar) : yield(n(t))
                  (t) : do(loop, t)
            loop(t)
      val vs = unique $ freevars(t)
      val n = def-type-obj(t, vs)
      [n, vs]
   defn type-obj (t:KType) :
      val [n, vs] = make-type-obj(t)
      KTypeObject(n, map(KVar, vs), false)
   defn ls-type-obj (t:KType) :
      val [n, vs] = make-type-obj(t)
      KLSTypeObject(n, map(KLSVar, vs))
         

   ;Convert all expressions
   defn conv-exp (e:KExp) :
      match(mapr(conv-exp, e)) :
         (e:KDo) :
            val targs* = map(type-obj, targs(e))
            val args* = append(targs*, args(e))
            KCall(false, length(args(e)), func(e), args*, false)
         (e) : e
   defn conv-ls-exp (e:KLSExp) :
      match(mapr(conv-ls-exp, e)) :
         (e:FuncExp) :
            FuncExp(map(conv-exp, func(e)))
         (e:KLSCall) :
            val targs* = map(ls-type-obj, targs(e))
            val args* = append(targs*, args(e))
            KLSWCall(false, length(args(e)), func(e), args*, type(e))
         (e:KLSCallClosure) :
            val targs* = map(ls-type-obj, targs(e))
            val args* = append(targs*, args(e))
            KLSWCallClosure(false, length(args(e)), func(e), args*, type(e))
   defn conv-ls-comm (e:KLSComm) :
      mapr(conv-ls-exp, e)

   ;Driver
   for c in comms(prog) do :
      add{accum, _} $
      map{conv-exp, _} $
      map{conv-ls-comm, _} $
      c
   KProg(to-list(accum))


;                    Free Variable Computation
;                    =========================

defstruct FVEntry :
   args: List<Int>

defn freevar-table (prog:KProg) :
   ;Scan globals
   val globals = HashTable<Int, True>({_})
   defn global? (n:Int) : key?(globals, n)
   for c in comms(prog) do :
      match(c) :
         (c:KDef|KDefVar|KDefn|KDefmulti|KLSDef|KLSDefVar|KLSDefn|KDefConst) :
            globals[n(c)] = true
         (c) : false         

   ;Table
   val table = HashTable<Int, FVEntry>({_})

   ;Get the freevars in a given function
   defn freevars (f:Func) :
      val arg-vars = HashTable<Int,True|False>({_})
      
      defn mark-free (t:KType) :
         match(t) :
            (t:KTVar) : arg-vars[n(t)] = true
            (t) : do(mark-free, t)
      
      defn mark-free (e:KExp) :
         defn mark-func (f:Func) :
            for x in args(freevars(f)) do :
               arg-vars[x] = true               

         defn mark-branch (b:KBranch) :
            mark-free(body(b))
            for x in args(b) do :
               arg-vars[x] = false               
               
         match(e) :
            (e:KLet) :
               mark-free(body(e))
               match(def(e)) :
                  (d:KDef|KDefVar) :
                     do(mark-free, d)
                     arg-vars[n(d)] = false
                  (d:KDefn) :
                     mark-func(value(d))
                     arg-vars[n(d)] = false
            (e:KLetRec) :
               mark-free(body(e))
               for d in defs(e) do :
                  mark-func(value(d as KDefn))
               for d in defs(e) do :
                  arg-vars[n(d)] = false
            (e:KDispatch) :
               do(mark-free, args(e))
               do(mark-branch, branches(e))
            (e:KMatch) :
               do(mark-free, args(e))
               do(mark-branch, branches(e))
            (e:KNew) :
               for m in methods(e) do :
                  mark-func(func(m))
            (e:KVar) :
               if not global?(n(e)) :
                  arg-vars[n(e)] = true
            (e:KCast) :
               do(mark-free, e)
               mark-free(targ(e))
            (e) :
               do(mark-free, e)

      defn mark (f:Func) :
         match(f) :
            (f:KFn) :
               mark-free(body(f))
               for x in args(f) do :
                  arg-vars[x] = false
               for x in targs(f) do :
                  arg-vars[x] = false
            (f:KMultifn) :
               do(mark, funcs(f))

      defn true-keys (t:HashTable<Int,True|False>) :
         to-list $
         stream{key, _} $
         filter(value, t)
         
      mark(f)
      val fv = FVEntry $
         to-list $
         stream{key, _} $
         filter(value, arg-vars)
      table[n(f)] = fv   
      fv   

   ;Compute freevars for every expression
   defn scan-exp (e:KExp) :
      do(scan-comm, e)
      do(scan-exp, e)
   defn scan-comm (c:KComm) :
      match(c) :
         (c:KDefn) : freevars(value(c))
         (c:KDefmethod) : freevars(func(c))
         (c) : do(scan-exp, c)
   defn scan-ls-exp (e:KLSExp) :   
      match(e) :
         (e:FuncExp) : freevars(func(e))
         (e) : dor(scan-ls-exp, e)
   defn scan-ls-comm (c:KLSComm) :
      dor(scan-ls-exp, c)

   ;Driver
   for c in comms(prog) do :
      do(scan-ls-comm, c)
      do(scan-exp, c)
   table




