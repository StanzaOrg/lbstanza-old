defn make-var (t:tgt-TgtType) : make-var(VAR-TABLE!(), t)
defn make-vars (ts:List<tgt-TgtType>) : map(make-var, ts)
defn env-vars (n:Int) : VAR-TABLE!()[n]
defn var-types (n:Int) : types(VAR-TABLE!(), n)






