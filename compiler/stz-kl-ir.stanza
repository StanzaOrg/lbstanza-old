         args: List<KExp>  
      KCheckVoid :
         exp: KExp
         info: False|FileInfo
      KObject :
         n: Int
         args: List<KExp>
         info: False|FileInfo
      KObjectGet :
         object: KExp
         index: Int
      KTypeObject :
         n: Int
         args: List<KExp>
         info: False|FileInfo
      KClosure :
         n: Int
         args: List<KExp>
         info: False|FileInfo
      KClosureGet :
         exp: KExp
         index: Int
      KCall :
         tail?: True|False
         arity: Int
         func: Int
         args: List<KExp>
         info: False|FileInfo
      KCallDeref :
         tail?: True|False
         arity: Int
         func: KExp
         args: List<KExp>
         info: False|FileInfo
      KBox :
         value: False|KExp
      KBoxGet :
         box: KExp
      KBoxSet :
         box: KExp
         value: KExp
      KBranchError :
