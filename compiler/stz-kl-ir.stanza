public defn children (t:KType) -> Collection<KType> :
   val ts = Vector<KType>(4)
   do(add{ts, _}, t)
   ts

public defn subexps (e:KExp) -> Collection<KExp> :
   val es = Vector<KExp>(4)
   do(add{es, _}, e)
   es