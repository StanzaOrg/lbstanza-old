         info: False|FileInfo


   ;     KForm LoStanza Types
   ;     --------------------
   deftype KLSType
   defnodes KLSType :
      KByteT
      KIntT
      KLongT
      KFloatT
      KDoubleT
      KUnknownT
      KPtrT: (type:KLSType)
      KRefT: (type:KType)
      KFnT: (a:List<KLSType>, r:False|KLSType, b:KLSType)
      KStructT: (n:Int)


   ;     KForm Location
   ;     --------------
   deftype KLoc
   defnodes KLoc :
      Value: (exp:KLSExp)
      Deref: (exp:KLSExp, type:KLSType)
      Deptr: (exp:KLSExp, type:KLSType)
      Slot: (exp:KLSExp, type:KLSType, index:KLSExp)
      Field: (base:KLoc, n:Int) ;TODO: Should include type to make things easier


   ;     KForm LoStanza Commands
   ;     -----------------------
   deftype KLSComm
   defnodes KLSComm :
      KLSIgnoreComm :
         exp: KLSExp
      KLSSet :
         exp: KLoc
         value: KLSExp
      KLSLabels :
         blocks: List<KLSLabeledBlock>
      KLSLabel :
         n: Int
      KLSLabeledBlock :
         n: Int
         args: List<Int>
         types: List<KLSType>
         body: KLSComm
      KLSGoto :
         n: Int
         args: List<KLSExp>
      KLSReturn :
         exp: KLSExp
      KLSDefLocal :
         n: Int
         value: KLSExp
      KLSDefLocalVar :
         n: Int
         type: KLSType
         value: False|KLSExp
      KLSSeq :
         a: KLSComm
         b: KLSComm
      KLSIf :
         pred: KLSExp
         conseq: KLSComm
         alt: KLSComm
      KLSMatch :
         args: List<KLSExp>
         branches: List<KLSBranch>
         info: False|FileInfo
      KLSDispatch :
         args: List<KLSExp>
         branches: List<KLSBranch>
         info: False|FileInfo
      KLSSkip
      KLSBranchError :
         amb?: True|False
         info: False|FileInfo

   ;     KForm LoStanza Expressions
   ;     --------------------------
   deftype KLSExp
   defnodes KLSExp :
      KLSLet :
         comm: KLSComm
         exp: KLSExp
      KLSVar :
         n: Int
      KLSRef :
         n: Int
         args: List<KLSExp>
         info: False|FileInfo
      KLSRefArray :
         n: Int
         args: List<KLSExp>
         info: False|FileInfo
      KLSStruct :
         n: Int
         args: List<KLSExp>
      KLSPtr :
         exp: KLoc
      KLSRead :
         exp: KLoc
      KLSPrim :
         op: KLSOp
         args: List<KLSExp>
         type: KLSType
      KLSCall :
         func: KLSExp
         args: List<KLSExp>
         type: KLSType
         new-stack?: True|False
         info: False|FileInfo
      KLSSwitchStack :
         func: KLSExp
         arg: KLSExp
         type: KLSType
         info: False|FileInfo
      KLSCallC :
         func: KLSExp
         args: List<KLSExp>
         type: KLSType
         info: False|FileInfo
      KLSSizeof :
         type: KLSType
      KLSTagof :
         n: Int
      KLSConv :
         exp: KLSExp
         type: KLSType
         info: False|FileInfo
      KLSInterpret :
         exp: KLSExp
         type: KLSType      
      KLSLitByte :
         value: Byte
      KLSLitInt :
         value: Int
      KLSLitLong :
         value: Long
      KLSLitFloat :
         value: Float
      KLSLitDouble :
         value: Double
      KLSLitString :
         value: String
      KLSMix :
         exps: List<KLSCurry>
      KLSCurry :
         n: Int
         targs: List<KType>
         a1: List<KLSType>
         a2: KLSType
      KLSFn :
         ref: KExp

      ;     Working IR
      ;     ----------
      KLSCast :
         exp: KLSExp
         type: KType
         info: False|FileInfo
      KLSTypeObject :
         n: Int
         args: List<KLSExp>
         info: False|FileInfo
      FuncExp :
         func: Func
      KLSClosure :
         n: Int
         args: List<KLSExp>
         info: False|FileInfo
      KLSWCall :
         tail?: True|False
         arity: Int
         func: Int
         args: List<KLSExp>
         type: KLSType
         new-stack?: True|False
         info: False|FileInfo
      KLSWCallDeptr :
         tail?: True|False
         arity: Int
         func: KLSExp
         args: List<KLSExp>
         type: KLSType
         new-stack?: True|False
         info: False|FileInfo
      KLSWCallDeref :
         tail?: True|False
         arity: Int
         func: KLSExp
         args: List<KLSExp>
         type: KLSType
         new-stack?: True|False
         info: False|FileInfo
      KLSWCallC :
         func: Int
         args: List<KLSExp>
         type: KLSType
      KLSWCallCDeptr :
         func: KLSExp
         args: List<KLSExp>
         type: KLSType
      KLSWSwitchStack :
