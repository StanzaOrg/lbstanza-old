      if tail?(f) : ($fn* (targs ...) (a1 ...) a2 closure (args ...) body)
      else : ($fn (targs ...) (a1 ...) a2 closure (args ...) body)
   KMultifn :
      ($multi funcs ...)

   ;Working IR
   ;----------
   KCode :
      ($code arity (args ...) body)


